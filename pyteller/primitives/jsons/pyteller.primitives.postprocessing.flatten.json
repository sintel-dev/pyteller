{
    "name": "pyteller.primitives.postprocessing.flatten",
    "contributors": [
        "Skylar Eiskowitz <eiskowit@mit.edu>"
    ],
    "description": "flattens predictions and averages duplicate predicted values",
    "classifiers": {
        "type": "postprocessor",
        "subtype": "flatten"
    },
    "modalities": [],
    "primitive": "pyteller.primitives.postprocessing.flatten",
    "produce": {
        "args": [
            {
                "name": "X",
                "type": "pandas.DataFrame"
            },
            {
                "name": "index",
                "type": "Index or array-like"
            },
            {
                "name": "pred_length",
                "type": "int"
            },
            {
                "name": "freq",
                "type": "int"
            }
        ],
        "output": [
            {
                "name": "X",
                "type": "pandas.DataFrame"
            },
            {
                "name": "index",
                "type": "Index or array-like"
            }
        ]
    },
        "hyperparameters": {
            "fixed": {
                "pred_length": {
                    "type": "int",
                    "default": 1
                }
            }
        }
    }


