{
    "name": "pyteller.primitives.estimators.Persistence",
    "contributors": ["Skylar Eiskowitz <eiskowit@mit.edu>"],
    "documentation": "",
    "description": "Simple Estimator that always returns the last value as the forecasted value.",
    "classifiers": {
        "type": "estimator",
        "subtype": "regressor"
    },
    "modalities": [],
    "primitive": "pyteller.primitives.estimators.Persistence",
    "fit": {
        "method": "fit",
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            },
            {
                "name": "y",
                "type": "ndarray"

            }
        ],
        "output": [
            {
                "name": "val",
                "type": "float"
            }
        ]
    },
    "produce": {
        "method": "predict",
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "y",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
                "pred_length": {
                    "type": "int",
                    "default": "1"
                },
                "offset": {
                    "type": "int",
                    "default": "1"
                }
        }
    }
}
